{
  "title": "Blueberry Muffins",
  "yield": "15",
  "ingr": [
    "2 cups Baking Flour",
    "2 teaspoons baking powder",
    "1/2 teaspoon salt",
    "1/2 cup butter, softened",
    "1 cup sugar",
    "2 large eggs",
    "1 teaspoon vanilla extract",
    "1/2 cup milk",
    "2 cups blueberries"
  ]
}
